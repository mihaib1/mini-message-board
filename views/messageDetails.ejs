<link rel="stylesheet" href="styles.css">

<div class="message-container">
    <li>
        <% if(message) { %>
            <div class="user"> <b> <%= message.user %>: </b> </div> 
            <div class="message"> <%= message.text %> </div>
            <div class="date"> <%= message.added %></div>
        <% } %>

    </li>

</div>